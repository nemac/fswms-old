    { 'path' : '/flood/MODIS/CONUS_Phenological_Parameters2/2003/17_2003_phen_small_integral.img',                           'minColor' : '0,0,0',  'maxColor' : '255,255,255'}
    ];

def system(cmd):
    print cmd
    os.system(cmd)

for file in files:
    m = re.match(r'.*/(.+)\.img$', file['path'])
    base = m.group(1)
    outputfile = "cmaps/" + base + ".cmap"
    if 'minColor' in file:
        #system("./makestretchcmap -N 32 -a %s/%s --rgbMin %s --rgbMax %s > %s" % ( basedir, file['path'], file['minColor'], file['maxColor'], outputfile ))
        system("./makestretchcmap -N 32 -a %s --rgbMin %s --rgbMax %s > %s" % ( file['path'], file['minColor'], file['maxColor'], outputfile ))
    else:
        system("touch %s" % (outputfile))
