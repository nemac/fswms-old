def system(cmd):
    print cmd
    os.system(cmd)

for file in files:
    m = re.match(r'.*/(.+)\.img$', file['path'])
    base = m.group(1)
    if 'minColor' in file:
        #system("./makestretchcmap -N 32 -a %s/%s --rgbMin %s --rgbMax %s > %s" % ( basedir, file['path'], file['minColor'], file['maxColor'], outputfile ))
        system("./makestretchcmap -N 32 -a %s --rgbMin %s --rgbMax %s > cmaps/%s.cmap" % ( file['path'], file['minColor'], file['maxColor'],  base))
        system("./makestretchicon -T 0 -W 128 -h 15 -f 8 -L %s  -H %s -l low -r high cmapicons/%s.png" % (file['minColor'], file['maxColor'], base))
    else:
        system("touch cmaps/%s.cmap"    % (base))
        system("touch cmapicons/%s.png" % (base))
